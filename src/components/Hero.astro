---
import { Image } from "astro:assets";
import Banner from "../assets/hero_banner.jpg";
---

<div class="relative w-full h-full">
  <Image
    style={{ width: "100vw", height: "80vh", objectFit: "cover" }}
    src={Banner}
    class="brightness-75"
    alt="Logo"
  />
  <h1
    class="absolute inset-0 text-7xl flex items-center justify-center font-serif text-white z-10"
  >
    Services
  </h1>
</div>
